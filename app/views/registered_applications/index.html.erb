<div class="container body">
<h1 id="h1-align">My Registered Applications</h1>
<br>
<div class="table-responsive">
<table class="align table-striped table-hover table-condensed">
  <thead>
    <tr>
      <th id="th-style">Application</th>
      <th id="th-style">URL</th>
    </tr>
  </thead>

  <tbody>
    <% @registered_applications.each do |registered_application| %>
      <tr>
        <td><strong><%= registered_application.name %></td></strong>
        <td><%= link_to registered_application.url %></td>
        <td><%= link_to 'View', registered_application, class: 'btn btn-primary btn-xs' %></td>
        <td><%= link_to 'Edit', edit_registered_application_path(registered_application), class: 'btn btn-info btn-xs' %></td>
        <td><%= link_to 'Delete', registered_application, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger btn-xs' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

<br>

<%= link_to "Add New Application", new_registered_application_path, class: 'btn btn-primary' %>
</div>